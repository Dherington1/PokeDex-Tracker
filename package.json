{
  "name": "pokedex-tracker",
  "version": "1.0.0",
  "description": "track player caught pokemon progression",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/index.js",
